@model Vidly.Models.Customer

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>멤버십:@Model.MembershipType.Name</li>
    @if (Model.Birthdate.HasValue)
    {

        <li>생일:@Model.Birthdate.Value.ToString("yyyy/MM/dd")</li>
    }
</ul>
