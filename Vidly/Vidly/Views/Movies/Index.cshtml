@model IEnumerable<Vidly.Models.Movie>

@{
    ViewBag.Title = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>영화</h2>

<div>
    <table class="table">
        <tr>
            <th>영화제목</th>
            
        </tr>
        <tbody>
            @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Name</td>
        </tr>
}
        </tbody>
        

    </table>
</div>
